{
  "name": "react-webgl-starter",
  "version": "1.0.0",
  "description": "Starter boilerplate for front end React and WebGL 2.0 projects",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --repoprter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mstallmo/react-webgl-starter.git"
  },
  "keywords": [
    "React",
    "WebGL",
    "Babel",
    "Webpack"
  ],
  "author": "Mason Stallmo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mstallmo/react-webgl/issues"
  },
  "homepage": "https://github.com/mstallmo/react-webgl#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "gl-matrix": "^2.4.0",
    "jquery": "^3.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "compression": "^1.7.1",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^21.2.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.6",
    "jsdom": "^11.3.0",
    "json-loader": "^0.5.7",
    "mocha": "^4.0.1",
    "nock": "^9.0.24",
    "npm-run-all": "^4.1.1",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
